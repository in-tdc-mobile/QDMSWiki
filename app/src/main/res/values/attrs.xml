<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="fontface" format="string" />
        <attr name="fontColor" format="color" />
    </declare-styleable>

    -<declare-styleable name="BottomNavigationBehavior_Params">

    <attr name="tabLayoutId" format="reference"/>
    <attr name="selectedBackgroundVisible" format="boolean"/>
    <attr name="translucentNavigationEnabled" format="boolean"/>
    <attr name="colored" format="boolean"/>
    <attr name="accentColor" format="color"/>
    <attr name="inactiveColor" format="color"/>
    <attr name="disableColor" format="color"/>
    <attr name="coloredActive" format="color"/>
    <attr name="coloredInactive" format="color"/>

</declare-styleable>

    <declare-styleable name="LinearLayoutAnimation">
        <attr name="startAnimation" format="boolean" />
        <attr name="startRecursiveAnimation" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomProgressBar">
        <attr name="interval" format="integer" />
        <attr name="animationDuration" format="integer" />
        <attr name="indicatorHeight" format="integer" />
        <attr name="indicatorColor" format="color" />
        <attr name="indicatorRadius" format="integer" />
    </declare-styleable>

    <declare-styleable name="ScreenshotHighlightButton">
        <attr name="fill_color" format="color" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ShaderImageView">
        <attr name="siSquare" format="boolean" />
        <attr name="siBorderColor" format="color" />
        <attr name="siBorderWidth" format="dimension" />
        <attr name="siBorderAlpha" format="float" />
        <attr name="siForeground" format="integer|reference" />
        <!-- Rounded Image View -->
        <attr name="siRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ProgressBarCustom">
        <attr name="startColor" format="color"/>
        <attr name="endColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="amount" format="integer"/>
        <attr name="animationDirection" format="enum">
            <enum name="right" value="1"/>
            <enum name="left" value="-1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ArcProgress"><attr format="integer" name="arc_progress"/><attr format="float" name="arc_angle"/><attr format="dimension" name="arc_stroke_width"/><attr format="integer" name="arc_max"/><attr format="color" name="arc_unfinished_color"/><attr format="color" name="arc_finished_color"/><attr format="dimension" name="arc_text_size"/><attr format="color" name="arc_text_color"/><attr format="string" name="arc_suffix_text"/><attr format="dimension" name="arc_suffix_text_size"/><attr format="dimension" name="arc_suffix_text_padding"/><attr format="string" name="arc_bottom_text"/><attr format="dimension" name="arc_bottom_text_size"/></declare-styleable>
    <declare-styleable name="CircleProgress"><attr format="integer" name="circle_progress"/><attr format="integer" name="circle_max"/><attr format="color" name="circle_unfinished_color"/><attr format="color" name="circle_finished_color"/><attr format="dimension" name="circle_text_size"/><attr format="color" name="circle_text_color"/><attr format="string" name="circle_prefix_text"/><attr format="string" name="circle_suffix_text"/></declare-styleable>
    <declare-styleable name="DonutProgress"><attr format="integer" name="donut_progress"/><attr format="integer" name="donut_max"/><attr format="color" name="donut_unfinished_color"/><attr format="color" name="donut_finished_color"/><attr format="dimension" name="donut_finished_stroke_width"/><attr format="dimension" name="donut_unfinished_stroke_width"/><attr format="dimension" name="donut_text_size"/><attr format="color" name="donut_text_color"/><attr format="string" name="donut_prefix_text"/><attr format="string" name="donut_suffix_text"/><attr format="string" name="donut_text"/><attr format="color" name="donut_background_color"/><attr format="string" name="donut_inner_bottom_text"/><attr format="dimension" name="donut_inner_bottom_text_size"/><attr format="color" name="donut_inner_bottom_text_color"/><attr format="integer" name="donut_circle_starting_degree"/><attr format="boolean" name="donut_show_text"/><attr format="reference" name="donut_inner_drawable"/></declare-styleable>
    <declare-styleable name="Themes"><attr format="reference" name="circleProgressStyle"/><attr format="reference" name="donutProgressStyle"/><attr format="reference" name="arcProgressStyle"/></declare-styleable>

</resources>